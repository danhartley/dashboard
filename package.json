{
  "name": "dashboard",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@cucumber/pretty-formatter": "^1.0.0-alpha.2",
    "@reach/tabs": "^0.16.4",
    "@testing-library/cypress": "^8.0.2",
    "@testing-library/jest-dom": "^5.16.3",
    "@testing-library/react": "^12.1.4",
    "@types/node": "^16.11.26",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "cypress": "^9.5.3",
    "gh-pages": "^3.2.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-query": "^3.34.19",
    "react-router-dom": "^6.3.0",
    "react-scripts": "^5.0.0",
    "source-map-explorer": "^2.5.2",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "eject": "react-scripts eject",
    "start": "react-scripts start",
    "test": "react-scripts test . --detectOpenHandles",
    "test:debug": "react-scripts --inspect-brk test --runInBand --no-cache",
    "coverage": "react-scripts test --coverage .",
    "build": "react-scripts build",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "cucumber": "./node_modules/.bin/cucumber-js cypress/integration/dashboard/**/*.feature --import cypress/support/step_definitions/**/*.js",
    "cypress:open": "cypress open",
    "prepare": "husky install",
    "predeploy": "npm run build",
    "deploy": "gh-pages -b main -d build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@axe-core/react": "^4.4.2",
    "@cucumber/cucumber": "^8.0.0",
    "@testing-library/dom": "^8.12.0",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^14.1.1",
    "@types/jest": "^27.4.1",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "autoprefixer": "^10.4.4",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-plugin-react": "^7.29.4",
    "husky": "^7.0.0",
    "jest": "^27.5.1",
    "msw": "^0.39.2",
    "postcss": "^8.4.12",
    "prettier": "2.6.2",
    "pretty-quick": "^3.1.3",
    "tailwindcss": "^3.0.23",
    "ts-jest": "^27.1.4",
    "typescript": "^4.6.3"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "index.tsx",
      "reportWebVitals.ts",
      "/src/mocks",
      "enums.ts",
      "interfaces.ts",
      "types.ts",
      "webpack.config.js",
      "simple.tsx",
      "simple-child.tsx",
      "api-browser-client.ts"
    ]
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": false
  },
  "proxy": "https://danhartley.github.io/",
  "homepage": "https://danhartley.github.io/dashboard/"
}
